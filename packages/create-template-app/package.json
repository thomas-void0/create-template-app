{
  "name": "create-template-app",
  "author": "thomas-void0",
  "version": "0.1.12-beta",
  "description": "用于快速生成开发项目模板",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "format": "esm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomas-void0/create-template-app.git"
  },
  "homepage": "https://github.com/thomas-void0/create-template-app#readme",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "create-template-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "cta-template": "2.0.2",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0"
  }
